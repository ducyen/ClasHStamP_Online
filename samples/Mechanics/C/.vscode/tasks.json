{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/*.c", "${workspaceFolder}/Model/*.c", "${workspaceFolder}/NonOOP/*.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I/home/linuxbrew/.linuxbrew/include", "-I${workspaceFolder}", "-I${workspaceFolder}/Model", "-I${workspaceFolder}/NonOOP", "-I${workspaceFolder}/include", "-I/usr/local/include/SDL2", "-I/usr/include/SDL2", "-I.",
                "-L/home/linuxbrew/.linuxbrew/lib", "-L/usr/local/lib", "-lSDL2", "-lSDL2_gfx", "-lSDL2_image", "-lSDL2_mixer", "-lSDL2_ttf", "-lm", "-lchipmunk"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}